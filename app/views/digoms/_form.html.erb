  </div>
  <div class="field">
    <%= f.label :מפעל %><br>
    <%= f.text_field :factory %>
  </div>
  <div class="field">
    <%= f.label :נוכחים %><br>
    <%= f.text_field :persents %>
  </div>
  <div class="field">
    <%= f.label :"כמות זרמים" %><br>
    <%= f.number_field :numOfFlows %>
  </div>
  <div class="field">
    <%= f.label :"אופי זרמים" %><br>
    <%= f.text_field :flowType %>
  </div>
  <div class="field">
    <%= f.label :הערות %><br>
    <%= f.text_field :notes %>
  </div>
  <div class="field">
