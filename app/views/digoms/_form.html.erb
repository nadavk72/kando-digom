<%= form_for(@digom) do |f| %>
  <% if @digom.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@digom.errors.count, "error") %> prohibited this digom from being saved:</h2>

      <ul>
      <% @digom.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  
  <div class="field">
    <%= f.label :תאגיד %><br>
    <%=  select(:digom, :corp, Corporate.find(:all, :order=>:name).collect{ |c| [c.name,c.name]} , {}, {:"data-native-menu" => "false"}) %>
  </div>
  <div class="field">
    <%= f.label :מפעל %><br>
    <%=  select(:digom, :factory, Factory.find(:all, :order=>:name).collect{ |c| [c.name,c.name]} , {}, {:"data-native-menu" => "false"}) %>
  </div>

  <div class="field">
    <%= f.label :"תאריך דיגום" %><br>
    <%= f.date_field :d_date  %>
  </div>

<div class="field">
    <%= f.label :"שעת דיגום" %><br>
    <%= f.time_field :t_time  %>
  </div>

  

  <div class="field">
    <%= f.label :"קריאת מונה" %><br>
    <%= f.text_field :counter_call %>
  </div>
  <div class="field">
    <%= f.label :"שם דוגם" %><br>
    <%= f.text_field :owner %>
  </div>

  <div class="field">
    <%= f.label :נוכחים %><br>
    <%= f.text_field :persents %>
  </div>

  <div class="field">
    <%= f.label :"כמות זרמים" %><br>
    <%= f.number_field :numOfFlows %>
  </div>
  <div class="field">
    <%= f.label :"אופי זרמים" %><br>
    <%= f.text_field :flowType %>
  </div>  
  <div class="field">
    <%= f.label :הערות %><br>
    <%= f.text_field :notes %>
  </div>
  <div class="field">
    <%= f.label :מעבדה %><br>
    <%= f.text_field :lab %>
  </div>
  <div class="field">
    <%= f.label :"שם דוגם" %><br>
    <%= f.text_field :samplerName %>
  </div>
  <div class="field">
    <%= f.label :פרמטרים %><br>
    <%= f.text_field :parameters %>
  </div>
  <div class="field">
    <%= f.label :"לא בוצע דיגום" %><br>
    <%= f.check_box :executed %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
