<%= form_for(@digom) do |f| %>
  <% if @digom.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@digom.errors.count, "error") %> prohibited this digom from being saved:</h2>

      <ul>
      <% @digom.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div class="field">
    <%= f.label :"שם דיגום" %><br>
