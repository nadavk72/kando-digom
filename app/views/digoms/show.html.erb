<div data-role="header">
  <h1><%= @digom.corp %></h1>
  <%= link_to 'Home', digoms_url, "class" => "ui-btn-right" %>
</div>

<div data-role="content">

<p>
  <strong>שם דיגום:</strong>
  <%= @digom.digomId %>
</p>

<p>
  <strong>תאגיד:</strong>
  <%= @digom.corp %>
</p>

<p>
  <strong>מפעל:</strong>
  <%= @digom.factory %>
</p>

<p>
  <strong>תאריך דיגום:</strong>
  <%= @digom.d_date %>
</p>

<p>
  <strong>שעת דיגום:</strong>
  <%= @digom.t_time.strftime('%H:%M') %>

</p>

<p>
  <strong>קריאת מונה:</strong>
  <%= @digom.counter_call %>
</p>

<p>
  <strong>שם דוגם:</strong>
  <%= @digom.owner %>
</p>

<p>
  <strong>נוכחים:</strong>
  <%= @digom.persents %>
</p>

<p>
  <strong>כמות זרמים:</strong>
  <%= @digom.numOfFlows %>
</p>

<p>
  <strong>אופי הזרם:</strong>
  <%= @digom.flowType %>
</p>

<p>
  <strong>הערות:</strong>
  <%= @digom.notes %>
</p>

<p>
  <strong>מעבדה:</strong>
  <%= @digom.lab %>
</p>

<p>
  <strong>שם דוגם:</strong>
  <%= @digom.samplerName %>
</p>

<p>
  <strong>פרמטרים:</strong>
  <%= @digom.parameters %>
</p>

<p>
  <strong>לא בוצע דיגום:</strong>
  <%= @digom.executed %>
</p>

</div>


