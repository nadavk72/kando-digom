<h1>Listing water_suppliers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @water_suppliers.each do |water_supplier| %>
      <tr>
        <td><%= water_supplier.name %></td>
        <td><%= link_to 'Show', water_supplier %></td>
        <td><%= link_to 'Edit', edit_water_supplier_path(water_supplier) %></td>
        <td><%= link_to 'Destroy', water_supplier, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Water supplier', new_water_supplier_path %>
